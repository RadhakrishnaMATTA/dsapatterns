import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	for (int i=0;i<4 ;i++)
	{
	    
	    for(int j=0; j<4; j++)
	    {
	        
	        System.out.print("1");
	    }
	    System.out.println("");
	}
	
	
	}
}
System.out.println("Hello World");
}
}	